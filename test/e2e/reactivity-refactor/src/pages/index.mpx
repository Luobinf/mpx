<template>
  <view>
    <text>{{ name }}|| {{ info.address }}</text>
    <view wx:for="{{ bookList }}">
      <text>{{ item }}</text>
    </view>
  </view>
</template>

<script> 
import { createPage, del, set } from '@mpxjs/core'

createPage({
  data: {
    name: 'jack',
    info: {
      address: 'shaoxing'
    },
    bookList: ['name1', 'name2', 'name3']
  },
  onLoad() {
    // this.name = 'ma'
    this.bookList.push('name4')
    set(this.info, 'address', 'hangzhou')
    del(this.bookList, 1)
  }
})
</script>

