{
  "transform": {
    "^.+\\.(js|jsx)?$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|styl)$": "identity-obj-proxy"
  },
  "testEnvironment": "jsdom",
  "testMatch": ["**/packages/**/?(*.)+(spec|test).[jt]s?(x)"],
  "setupFilesAfterEnv": ["./scripts/setupJestEnv.js"],
  "testPathIgnorePatterns": ["dist", "node_modules"]
}
